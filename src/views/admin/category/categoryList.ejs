<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <link
            rel="stylesheet"
            href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css"
        />
        <link
            rel="stylesheet"
            href="/static/styles/pages/category/categoryList.css"
        />
        <title>Document</title>
    </head>
    <body>
        <div class="menu-bar">
            <section>
                <div class="icons">
                    <i class="fa-solid fa-user"></i>
                    <a href="">
                        <i class="fa-solid fa-gear"></i>
                    </a>
                </div>
                <h1>닉네임</h1>
                <h2>@아이디</h2>
            </section>
            <section>
                <% category.forEach(function(category) { %>
                <div>
                    <a href="/client/category/<%= category.categoryId %>">
                        <p><%= category.title %></p>
                    </a>
                </div>
                <% }) %>
            </section>
            <p class="logout">
                로그아웃
                <i class="fa-solid fa-right-from-bracket"></i>
            </p>
        </div>
    </body>
</html>
